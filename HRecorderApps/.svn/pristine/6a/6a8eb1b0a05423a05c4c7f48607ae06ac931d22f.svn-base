.deviceNameLayout {
  width: 100%;
  height: 50px;
  font-size: 16px;
}
.deviceListLayout {
  width: 100%;
  background-color: #fff;
  border-radius: 10px;
  margin-top: 13px;
}

.deviceName {
  float: left;
  width: 85%;
  height: 49px;
  line-height: 49px;
  font-size: 16px;
  margin-left: 5%;
}

.goLayout {
  float: left;
  width: 10%;
  height: 49px;  
}
.go {
  width: 50%;
  height: 30px;
  margin-top: 10px
}
.line {
  width: 90%;
  height: 1px;
  background-color: #EAEAEA;
  margin-left: 5%;
  margin-right: 5%;
  float: left;
}

:global(.am-list-item) {     
    border: 1px solid #ccc;
    border-radius: 28px;
    min-height: 35px;
} 
:global(.am-input-item) {     
  height: 35px;
} 
:global(.am-navbar-left) {     
  padding-left: 0px;
} 
:global(.am-list-item .am-input-control input) {     
  font-size: 14px;
} 

:global(.am-search) {
  background-color: transparent;
  margin-top: 2%;
  margin-right: 2%;
  margin-bottom: -2%;
  margin-left: 2%;
}
:global(.am-search-input) {
  border: 1px solid #ddd;
  border-radius: 28px;
  height: 35px;
}
:global(.sub-title) {
  color: #888;
  font-size: 14px;
  padding: 30px 0 18px 0;
}
:global(.am-search-input .am-search-synthetic-ph) {
  height: 35px;
  line-height: 35px;
  text-align: left;
  margin-left: 20px;
}
:global(.am-search-input input[type="search"]) {
  height: 35px;
  margin-left: 21px;
}
:global(.am-search-input .am-search-clear) {
  padding-top: 10px;
  padding-right: 6.5px;
  padding-bottom: 10px;
  padding-left: 6.5px;
}