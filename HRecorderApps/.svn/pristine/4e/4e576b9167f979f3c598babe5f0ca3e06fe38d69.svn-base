.orderFailList {
    padding: 4%;
}

.title {
    text-align: center;
    margin: 4%;
    font-size: 16px;
}

.mark {
    color: #f00;
    margin-bottom: 4%;
}

.timeSelect {
    text-align: center;
    margin: 3%;
    color: rgb(33, 160, 50);
}

.stepBar {
    width: 100%;
    height: 100%;
    margin-top: 4%;
    :global(.am-steps-small .am-steps-item-icon) {
        width: 20%;
        height: 26px;
    }
}

.stepIcon {
    width: 50%;
    height: 100%;
    background-color: #ff0;
    border-radius: 50%;
}

.btns {
    position: absolute;
    width: 92%;
    bottom: 4%;

    :global(.am-button) {
        float: left;
        width: 46%;
        text-align: center;
        margin-right: 4%;
    }
}