.wrapperContent {
  :global(.am-navbar-title){
    display: block;
  }
  :global(.am-accordion .am-accordion-item .am-accordion-content){
    border: 1px solid #ccc;
  }
}

.listLyaout {
    width: 100%;
    height: 100%;
}
.nodataLayout {
    width: 100%;
    height: 100%;
    text-align: center;
}
.nodateImg {
  width: 65%;
  height: 10%;
  margin-top: 55%;
}

.nodataContent {
  margin-top: 10px;
  color: #ccc;
}

.deviceNameLayout {
  width: 100%;
  height:16vw;
  font-size: 16px;
}
.deviceListLayout {
  width: 100%;
  background-color: #fff;
  border-radius: 10px;
  margin-top: 13px;
  height: 100%;
}

.deviceName {
  float: left;
  width: 85%;
  height: 16vw;
  line-height: 16vw;
  font-size: 4vw;
  margin-left: 5%;
  color: #333333;
}

.goLayout {
  float: left;
  width: 10%;
  height: 16vw;  
}
.go {
  width: 55%;
  height: 30px;
  margin-top: 15px;
  color: #888;
}
.line {
  width: 90%;
  height: 1px;
  background-color: #E8E8E8;
  margin-left: 5%;
  margin-right: 5%;
  float: left;
}